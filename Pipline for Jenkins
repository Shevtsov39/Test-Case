pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                git credentialsId: 'github', url: 'https://github.com/Shevtsov39/Test-Case'
            }
        }
        stage('Running Robot') {
            steps {
                sh 'robot /home/vsevolod/Test-Case'
            }
        }    
    }
}
